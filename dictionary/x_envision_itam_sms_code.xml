        <element label="手机号码" max_length="40" name="mobile_phone" type="string"/>
        <element attributes="ignore_filter_on_new=true" default="javascript:global.getNextObjNumberPadded();" label="单号" max_length="40" name="number" read_only="true" type="string"/>
        <element label="请求ID" max_length="40" name="request_id" type="string"/>
        <element label="发送时间" max_length="40" name="send_time" type="glide_date_time"/>
        <element label="验证码" max_length="40" name="sms_code" type="string"/>
        <element label="使用时间" max_length="40" name="use_time" type="glide_date_time"/>
